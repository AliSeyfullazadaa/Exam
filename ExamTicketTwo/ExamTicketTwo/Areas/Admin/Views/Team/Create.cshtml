
@model TeamMemberGetDto

@{
    ViewData["Title"] = "Create";
}

<div class="main-panel">
    <div class="content-wrapper">
        <div class="row">
            <div class="col-md-6 grid-margin stretch-card">
                <div class="card">
                    <div class="card-body">
                        <h4 class="card-title">Create form</h4>
                        <div asp-validation-summary="ModelOnly" class="alert alert-danger alert-dismissible"></div>
                        <form class="forms-sample" method="post"  enctype="multipart/form-data">
                      @Html.AntiForgeryToken()
                            <div class="form-group">
                                <label asp-for="FullName">FullName</label>
                                <input asp-for="FullName" class="form-control" placeholder="FullName">
                                <span asp-validation-for="FullName" class="text-text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="Profession">Profession</label>
                                <input asp-for="Profession" class="form-control" placeholder="Profession">
                                <span asp-validation-for="Profession" class="text-text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="FacebookUrl">FacebookUrl</label>
                                <input asp-for="FacebookUrl" class="form-control" placeholder="Facebook">
                                <span asp-validation-for="FacebookUrl" class="text-text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="TwitterUrl">Twitter</label>
                                <input asp-for="TwitterUrl" class="form-control" placeholder="Twitter">
                                <span asp-validation-for="TwitterUrl" class="text-text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="InstagramUrl">Instagram</label>
                                <input asp-for="InstagramUrl" class="form-control" placeholder="Instagram">
                                <span asp-validation-for="InstagramUrl" class="text-text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="LinkedinUrl"></label>
                                <input asp-for="LinkedinUrl" class="form-control" placeholder="LinkedIn">
                                <span asp-validation-for="LinkedinUrl" class="text-text-danger"></span>
                            </div>
                            <div class="form-group">
                                <label asp-for="FormFile">FormFile</label>
                                <input asp-for="FormFile" class="form-control" placeholder="FormFile">
                                <span asp-validation-for="FormFile" class="text-text-danger"></span>
                              
                            </div>

                            <button type="submit" class="btn btn-primary me-2">Submit</button>
                            <button class="btn btn-light" asp-action="Index">Cancel</button>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


